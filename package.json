{
  "preferGlobal": true,
  "main": "index",
  "name": "mediatidy",
  "description": "A simple CLI tool to keep your media nice and tidy!",
  "version": "0.4.1",
  "author": "Daniel Bohannon <tkdan235@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:tkdan235/mediatidy.git"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "keywords": [
    "cli",
    "tidy",
    "organize",
    "tv shows",
    "movies"
  ],
  "dependencies": {
    "async": "^2.6.0",
    "coffee-script": "^1.10.0",
    "colors": "^1.1.2",
    "commander": "git://github.com/zhiyelee/commander.js.git#buginfo",
    "fs-extra": "^4.0.2",
    "lodash": "^4.17.4",
    "node-dir": "^0.1.11",
    "node-ffprobe": "^1.2.2",
    "pretty-bytes": "^4.0.2",
    "progress": "^2.0.0",
    "prompt": "^1.0.0",
    "sqlite3": "^3.1.13"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "name": "max_line_length",
      "value": 120,
      "level": "error",
      "limitComments": true
    }
  }
}
